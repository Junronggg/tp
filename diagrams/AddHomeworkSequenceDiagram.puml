@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":AddHomeworkCommandParser" as AddHomeworkCommandParser LOGIC_COLOR
participant ":AddHomeworkCommand" as AddHomeworkCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("add_hw i/E1234567 a/1")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("add_hw i/E1234567 a/1")
activate AddressBookParser

create AddHomeworkCommandParser
AddressBookParser -> AddHomeworkCommandParser
activate AddHomeworkCommandParser

AddHomeworkCommandParser --> AddressBookParser
deactivate AddHomeworkCommandParser

AddressBookParser -> AddHomeworkCommandParser : parse("i/E1234567 a/1")
activate AddHomeworkCommandParser

create AddHomeworkCommand
AddHomeworkCommandParser -> AddHomeworkCommand
activate AddHomeworkCommand

AddHomeworkCommand --> AddHomeworkCommandParser :
deactivate AddHomeworkCommand

AddHomeworkCommandParser --> AddressBookParser : d
deactivate AddHomeworkCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AddHomeworkCommandParser -[hidden]-> AddressBookParser
destroy AddHomeworkCommandParser

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

LogicManager -> AddHomeworkCommand : execute(m)
activate AddHomeworkCommand

AddHomeworkCommand -> Model : AddHomeworkCommand(E1234567, 1)
activate Model

Model --> AddHomeworkCommand
deactivate Model

create CommandResult
AddHomeworkCommand -> CommandResult
activate CommandResult

CommandResult --> AddHomeworkCommand
deactivate CommandResult

AddHomeworkCommand --> LogicManager : r
deactivate AddHomeworkCommand

[<--LogicManager
deactivate LogicManager
@enduml
